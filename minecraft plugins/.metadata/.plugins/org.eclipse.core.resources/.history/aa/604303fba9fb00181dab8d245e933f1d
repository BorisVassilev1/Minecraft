package org.CDnoMlqko.ChatUtils.enchantments;

import org.bukkit.enchantments.Enchantment;

public enum CustomEnchantmentRarity {
	
	COMMON(
			new String[]   {},
			new float[] {}),
	EPIC(
			new String[]   {},
			new float[] {}),
	LEGENDARY(
			new String[]   {},
			new float[] {});
	
	Enchantment[] enchantmentsPoll;
	float[] rarities;
	
	private CustomEnchantmentRarity(String[] names, float[] rarities) {
		this.enchantmentsPoll = new Enchantment[names.length];
		for(int i = 0; i < names.length; i ++)
		{
			enchantmentsPoll[i] = Enchantment.getByName(names[i]);
		}
	}

}
